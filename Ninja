local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ภูมิ hub", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main Memu")
Section:NewButton("Auto click", "Rejoin to stop", function()
while wait() do
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer()
end
end)

Section:NewButton("Auto best sword", "Rejoin to stop", function()
while wait() do
    local args = {
    [1] = "Area5",
    [2] = "Katana",
    [3] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EquipAndUnequipTool"):FireServer(unpack(args))
end
end)

Section:NewButton("Auto egg event", "Rejoin to stop", function()
while wait() do
    local args = {
    [1] = 1,
    [2] = {
        ["FlareChandelier"] = true,
        ["FlareNinja"] = true,
        ["FlareDeity"] = true,
        ["FlareSamurai"] = true
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EggEvent1"):FireServer(unpack(args))
end
end)
